#include<iostream>
using namespace std;
int main(){
    int a[5]={10,20,30,19,50};
    int n,f=0;
    cout<<"enter key to be searched::"<<endl;
    cin>>n;
    for(int i=0;i<5;i++){
        if(a[i]==n){
            cout<<n<<"is found at:"<<i<<endl;
            f=1;
            break;
        }
        else{
            i=i+1;
        }
    }
    if(f==0){
        cout<<"key is not found"<<endl;
    }
}
